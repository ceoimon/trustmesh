{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "semicolon": [
      true,
      "never"
    ],
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "ordered-imports": false,
    "no-console": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-unused-variable": true
  },
  "rulesDirectory": []
}
